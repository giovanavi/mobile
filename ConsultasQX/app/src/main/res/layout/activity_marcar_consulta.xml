<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.MarcarConsulta">

    <TextView
        android:id="@+id/textViewMarca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ConsultasQX"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="34dp" />

    <TextView
        android:id="@+id/textViewEspecialidade"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:gravity="left"
        android:text="Especialidade"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="127dp" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteEspecialidade"
        android:layout_width="180dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="199dp"
        tools:layout_editor_absoluteY="127dp" />

    <TextView
        android:id="@+id/textViewPagamento"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:gravity="left"
        android:text="Pagamento"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="173dp" />

    <AutoCompleteTextView
        android:id="@+id/autoCompletePagamento"
        android:layout_width="180dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="199dp"
        tools:layout_editor_absoluteY="173dp" />

    <TextView
        android:id="@+id/textViewConvenio"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:gravity="left"
        android:text="Convenio"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="219dp" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteConvenio"
        android:layout_width="180dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="199dp"
        tools:layout_editor_absoluteY="219dp" />

    <Button
        android:id="@+id/btnPesquisar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PESQUISAR"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="285dp" />

    <ListView
        android:id="@+id/listViewMedicos"
        android:layout_width="409dp"
        android:layout_height="361dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="369dp" />

</androidx.constraintlayout.widget.ConstraintLayout>